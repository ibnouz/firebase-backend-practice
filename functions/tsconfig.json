{
  "compilerOptions": {
    "skipLibCheck": true,
    "types": ["jest","node","supertest"],
    "resolveJsonModule": true,
    "module": "NodeNext",
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es6",//"es2017"
  },
  "compileOnSave": true,
  "include": [
    "src",
    "test",
    "src/**/*",
    "test/**/*.ts"
  ]
}
